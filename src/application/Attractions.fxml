<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.image.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:id="root" prefHeight="500.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.AttractionsController">
   <children>
      <ImageView fitHeight="500.0" fitWidth="800.0" pickOnBounds="true">
         <image>
            <Image url="@assets/images/Attractions_background.jpg" />
         </image>
      </ImageView>
      <ImageView fitHeight="89.0" fitWidth="343.0" layoutX="31.0" layoutY="20.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@assets/images/Attractions_header.png" />
         </image>
      </ImageView>
      <Text fill="WHITE" layoutX="52.0" layoutY="129.0" strokeType="OUTSIDE" strokeWidth="0.0" text="City (ex: San Antonio)">
         <font>
            <Font size="18.0" />
         </font>
      </Text>
      <TextField fx:id="cityInput" layoutX="52.0" layoutY="145.0" prefHeight="31.0" prefWidth="244.0" />
      <Button fx:id="searchBtn" layoutX="321.0" layoutY="145.0" mnemonicParsing="false" onMouseClicked="#handleSearch" text="Search" />
      <Text fill="WHITE" layoutX="168.0" layoutY="219.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Events">
         <font>
            <Font size="18.0" />
         </font>
      </Text>
      <TextArea fx:id="eventsField" layoutX="46.0" layoutY="243.0" prefHeight="200.0" prefWidth="375.0" />
      <Text fill="WHITE" layoutX="548.0" layoutY="152.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Monuments">
         <font>
            <Font size="18.0" />
         </font></Text>
      <TextArea fx:id="monumentsField" layoutX="455.0" layoutY="176.0" prefHeight="266.0" prefWidth="315.0" />
      <ImageView fx:id="backBtn" fitHeight="59.0" fitWidth="65.0" layoutX="695.0" layoutY="24.0" onMouseClicked="#showMain" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@assets/images/backBtn.png" />
         </image>
      </ImageView>
   </children>
</AnchorPane>
